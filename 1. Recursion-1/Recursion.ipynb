    "def sumOfNaturalNum():\n",
    "    num=int(input('enter number:'))\n",
    "    sum=0\n",
    "    for i in range(1,num+1):\n",
    "        sum+=i\n",
    "    return sum\n",
    "print(sumOfNaturalNum())"
